{
	"compilerOptions": {
		"lib": [
			"ESNext"
		],
		"module": "ESNext",
		"moduleResolution": "Node",
		"target": "ES2022",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"declaration": true,
		"allowJs": true,
		"allowImportingTsExtensions": true,
		"noEmit": true,
	},
	"include": [
		"scripts/**/*.ts",
		"src/**/*.ts"
, "src/lib/types.ts.tmp"	]
}